{
    "name": "tritium-syntax",
    "displayName": "tritium-syntax",
    "description": "tritium syntax highlight",
    "version": "0.0.1",
    "publisher": "william",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "tritium",
            "aliases": ["tritium", "tritium"],
            "extensions": [".ts"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "tritium",
            "scopeName": "source.ts",
            "path": "./syntaxes/tritium.tmLanguage.json"
        }]
    }
}